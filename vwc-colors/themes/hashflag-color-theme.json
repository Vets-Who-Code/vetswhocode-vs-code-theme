{
  "name": "HashFlag",
  "type": "hc-black",
  "colors": {
    "editor.background": "#091f40",
    "editor.foreground": "#d4d4d4",
    "editor.selectionBackground": "#216eb6",
    "editor.selectionForeground": "#ffffff",
    "editor.lineHighlightBackground": "#1f3e52",
    "editor.lineHighlightBorder": "#5f9cf8",
    "tab.activeBorder": "#559fff",
    "panel.border": "#559fff",
    "activityBar.background": "#1c394b",
    "activityBar.border": "#c5203e",
    "activityBarBadge.background": "#eb3d54",
    "sideBarSectionHeader.background": "#1b3647",
    "sideBar.background": "#10232e",
    "statusBar.background": "#091f40",
    "statusBar.foreground": "#ffffff"
  },
  "tokenColors": [
    {
      "settings": {
        "foreground": "#21e6e6"
      }
    },
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic, bold, underline, strikethrough",
        "foreground": "#21e6e6"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#f07178"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#c5203e"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.control",
        "constant.other.color",
        "punctuation",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#97d5ff"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#5f9cf8"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "fontStyle": "normal",
        "foreground": "#97d5ff"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "variable.parameter",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#ecfd55"
      }
    }
  ]
}